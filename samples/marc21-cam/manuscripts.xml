<marc:controlfield tag="008">020506m16661706enk     |    |||| ||lat||</marc:controlfield><marc:datafield tag="035" ind1=" " ind2=" "><marc:subfield code="9">2980003514</marc:subfield></marc:datafield><marc:datafield tag="041" ind1="0" ind2=" "><marc:subfield code="a">lat</marc:subfield><marc:subfield code="a">eng</marc:subfield></marc:datafield><marc:datafield tag="100" ind1="1" ind2=" "><marc:subfield code="a">Driffield, Stephen.</marc:subfield></marc:datafield><marc:datafield tag="245" ind1="1" ind2="0"><marc:subfield code="a">Attorney's notebook, ca. 1666-1706.</marc:subfield></marc:datafield><marc:datafield tag="260" ind1=" " ind2=" "><marc:subfield code="a">England,</marc:subfield><marc:subfield code="c">1666-1706.</marc:subfield></marc:datafield><marc:datafield tag="300" ind1=" " ind2=" "><marc:subfield code="a">1 v. (198p.).</marc:subfield></marc:datafield><marc:datafield tag="351" ind1=" " ind2=" "><marc:subfield code="c">Item.</marc:subfield></marc:datafield><marc:datafield tag="500" ind1=" " ind2=" "><marc:subfield code="a">Legal notebook of two attorneys, possibly Stephen Driffield, ca. 1666-1675, and George Fothergill of Ripon, ca. 1695-1706, containing a formulary and notes of cases in Yorkshire and Westminster. In Latin and English.</marc:subfield></marc:datafield><marc:datafield tag="500" ind1=" " ind2=" "><marc:subfield code="a">Brown cloth (19th century); old vellum cover.</marc:subfield></marc:datafield><marc:datafield tag="510" ind1="3" ind2=" "><marc:subfield code="a">Described in: Baker and Ringrose, _Catalogue of English Legal Manuscripts in Cambridge University Library_, pp. 598-9</marc:subfield></marc:datafield><marc:datafield tag="524" ind1=" " ind2=" "><marc:subfield code="a">Cambridge University Library, Add. MS 6235.</marc:subfield></marc:datafield><marc:datafield tag="541" ind1=" " ind2=" "><marc:subfield code="a">Whitaker, J.;</marc:subfield><marc:subfield code="c">Purchase;</marc:subfield><marc:subfield code="d">1920.</marc:subfield></marc:datafield><marc:datafield tag="650" ind1=" " ind2="0"><marc:subfield code="a">Law</marc:subfield><marc:subfield code="z">England</marc:subfield><marc:subfield code="x">History</marc:subfield><marc:subfield code="y">17th century</marc:subfield><marc:subfield code="x">Sources.</marc:subfield></marc:datafield><marc:datafield tag="651" ind1=" " ind2="0"><marc:subfield code="a">Ripon (England)</marc:subfield></marc:datafield><marc:datafield tag="651" ind1=" " ind2="0"><marc:subfield code="a">Yorkshire (England)</marc:subfield></marc:datafield><marc:datafield tag="700" ind1="1" ind2=" "><marc:subfield code="a">Fothergill, George,</marc:subfield><marc:subfield code="d">fl. 1700.</marc:subfield></marc:datafield><marc:datafield tag="830" ind1=" " ind2="0"><marc:subfield code="a">MS.Add.</marc:subfield></marc:datafield></marc:record>
